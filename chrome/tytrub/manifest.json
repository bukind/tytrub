{
  "name": "TyTrub Extensions",
  "description" : "Tear the tube Extension",
  "version": "0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "countdown.html",
    "default_icon": "tytrub.png"
  }
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js: ["js/contentscript.js"],
      "run_at": "document_start",
      "all_frames": false
    }
  ]
}
